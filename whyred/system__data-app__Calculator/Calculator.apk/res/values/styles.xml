    <style name="CalWidget.Divider.Pad" parent="@style/CalWidget.Divider" />
    <style name="CalWidget.Divider.Pad.H" parent="@style/CalWidget.Divider.Pad">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0px</item>
