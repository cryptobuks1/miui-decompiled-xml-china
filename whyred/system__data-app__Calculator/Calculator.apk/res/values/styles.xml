        <item name="android:minHeight">@dimen/martgage_item_height</item>
    </style>
    <style name="TextAppearance.Mortgage.Tab" parent="@miui:style/TextAppearance.WindowTitle.Subtitle">
        <item name="android:textColor">@color/mortgage_tab</item>
    </style>
    <style name="Widget.ActionBar.Title" parent="@miui:style/Widget.ActionBar">
        <item name="android:displayOptions">showCustom</item>
        <item name="android:customNavigationLayout">@layout/layout_title</item>
    </style>
    <style name="btn_row_style">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="button_equal_style" parent="@style/button_style">
        <item name="android:textSize">@dimen/button_fun_size</item>
        <item name="android:textColor">@color/button_equal_text_color</item>
    </style>
    <style name="button_fun_scientific_style" parent="@style/button_fun_style">
        <item name="android:textSize">@dimen/button_fun_scientific_size</item>
        <item name="android:background">@drawable/btn_fun</item>
    </style>
    <style name="button_fun_style" parent="@style/button_style">
        <item name="android:textSize">@dimen/button_fun_size</item>
        <item name="android:background">@drawable/btn_fun</item>
    </style>
