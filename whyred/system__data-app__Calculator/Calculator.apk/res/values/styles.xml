        <item name="miui:actionBarBackground">@color/grid_item_bg</item>
        <item name="miui:textColorBlankPage">false</item>
    </style>
    <style name="Calculator.Theme.Light" parent="@miui:style/Theme.Light" />
    <style name="Calculator.Theme.Light.ActionBar" parent="@style/Calculator.Theme.Light">
        <item name="miui:actionBarBackground">@color/actionbar_bg</item>
    </style>
    <style name="Calculator.Theme.Light.Base" parent="@style/Calculator.Theme.Light">
        <item name="android:windowBackground">@miui:drawable/window_bg_secondary_light</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:actionBarStyle">@style/Widget.ActionBar.Title</item>
        <item name="miui:startingWindowOverlay">@null</item>
        <item name="miui:actionBarBackground">@color/actionbar_bg</item>
    </style>
    <style name="Calculator.Theme.Light.Normal" parent="@style/Calculator.Theme.Light.Base">
        <item name="android:windowBackground">@miui:drawable/window_bg_secondary_light</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>
    <style name="Calculator.Theme.Light.Simple" parent="@style/Calculator.Theme.Light">
        <item name="miui:startingWindowOverlay">@null</item>
    </style>
    <style name="Calculator.Theme.Light.Translucent" parent="@style/Calculator.Theme.Light.Base">
        <item name="android:windowIsTranslucent">true</item>
    </style>
