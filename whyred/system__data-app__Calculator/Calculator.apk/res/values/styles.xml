        <item name="android:background">@null</item>
        <item name="android:paddingTop">@dimen/result_list_item_margin_top</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="CalWidget.TabView" parent="@miui:style/Widget.TabView">
        <item name="android:textAppearance">@style/TextAppearance.Mortgage.Tab</item>
        <item name="android:textSize">@dimen/tab_text_size</item>
        <item name="android:textColor">@color/tab_text</item>
        <item name="android:layout_width">@dimen/tab_width</item>
        <item name="android:layout_height">@dimen/tab_text_height</item>
    </style>
