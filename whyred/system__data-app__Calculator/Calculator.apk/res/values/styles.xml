        <item name="android:textSize">@dimen/button_scien_fun_size</item>
        <item name="android:background">@drawable/btn_fun</item>
    </style>
    <style name="button_style">
        <item name="android:textColor">@color/button_text</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="display_style">
        <item name="android:textSize">@dimen/display_text_size</item>
        <item name="android:textColor">@color/show_screen_display_color</item>
        <item name="android:gravity">end</item>
        <item name="android:paddingTop">0.0dip</item>
        <item name="android:paddingBottom">0.0dip</item>
        <item name="android:scrollbars">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:cursorVisible">false</item>
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:inputType">textNoSuggestions</item>
    </style>
