<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />
    <style name="ActionBarWhiteTitle" parent="@miui:style/Widget.ActionBar">
        <item name="android:background">@color/vsim_action_bar_bg_color</item>
        <item name="android:titleTextStyle">@style/TextAppearanceWindowTitle</item>
    </style>
    <style name="ActionBar_Search_Dark" parent="@miui:style/Widget.Dark.ActionBar">
        <item name="android:logo">@drawable/empty</item>
        <item name="android:displayOptions">useLogo</item>
    </style>
    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="DiagnosisWhiteTitle" parent="@miui:style/Widget.ActionBar">
        <item name="android:background">#00000000</item>
        <item name="android:titleTextStyle">@style/TextAppearanceWindowTitle</item>
        <item name="android:backgroundStacked">#ff346c36</item>
        <item name="android:backgroundSplit">#00000000</item>
    </style>
    <style name="Dialog_Anim_Style" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/translate_dialog_anim</item>
        <item name="android:windowExitAnimation">@anim/translate_dialog_anim</item>
    </style>
    <style name="Dialog_Window_Anim" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/translate_dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/translate_dialog_out_anim</item>
    </style>
    <style name="FullScreenTranslucent" parent="@android:style/Theme.Translucent">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="ListViewItem" parent="@miui:style/Widget.ListView.Item.SingleLine">
        <item name="android:descendantFocusability">blocksDescendants</item>
        <item name="android:layout_width">fill_parent</item>
    </style>
    <style name="MainActionbarStyle" parent="@miui:style/Widget.ActionBar">
        <item name="android:displayOptions">showCustom</item>
    </style>
    <style name="MiuiDialog" parent="@miui:style/Theme.Light.Dialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="Mm" />
    <style name="Mm.Light" parent="@miui:style/Theme.Light">
        <item name="android:actionBarStyle">@style/MmActionBar</item>
        <item name="miui:windowTranslucentStatus">transparentDark</item>
    </style>
    <style name="Mm.Transparent" parent="@miui:style/Theme.Dark.NoTitle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="Mm.Transparent.Web" parent="@style/Mm.Transparent">
        <item name="android:windowIsFloating">false</item>
    </style>
    <style name="MmActionBar" parent="@miui:style/Widget.ActionBar">
        <item name="android:background">@color/action_bar_bg_color</item>
        <item name="android:displayOptions">showCustom</item>
    </style>
    <style name="MmBtn">
        <item name="android:textColor">@color/mm_btn_text_color</item>
        <item name="android:background">@drawable/mm_btn_bg</item>
    </style>
    <style name="OnScreenHintTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/primary_text_dark</item>
        <item name="android:textColorHighlight">#ffff9200</item>
        <item name="android:textColorHint">#ff808080</item>
        <item name="android:textColorLink">#ff5c5cff</item>
    </style>
    <style name="OnScreenHintTextAppearance.Small" parent="@style/OnScreenHintTextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/secondary_text_dark</item>
    </style>
    <style name="PopupTitleSeparator">
        <item name="android:background">@color/oliveapp_camera_popup_title_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0dip</item>
    </style>
    <style name="PopupTitleText">
        <item name="android:textSize">@dimen/oliveapp_camera_popup_title_text_size</item>
        <item name="android:textColor">@color/oliveapp_camera_popup_title_color</item>
        <item name="android:layout_gravity">center|left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ReviewControlIcon">
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/oliveapp_camera_bg_pressed</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">@dimen/oliveapp_camera_switcher_size</item>
        <item name="android:layout_height">@dimen/oliveapp_camera_switcher_size</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="SampleAppTheme" parent="@android:style/Theme.Holo.Light.NoActionBar" />
    <style name="SearchEditTextStyle" parent="@android:style/Widget.EditText">
        <item name="android:singleLine">true</item>
        <item name="android:imeOptions">actionSearch|flagNoExtractUi</item>
    </style>
    <style name="SearchListViewItem" parent="@style/ListViewItem">
        <item name="android:background">@drawable/search_list_item_selector</item>
        <item name="android:paddingLeft">@dimen/search_list_item_padding</item>
        <item name="android:layout_height">@dimen/search_list_item_height</item>
    </style>
    <style name="SettingItemList">
        <item name="android:layout_gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingBottom">3.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:listSelector">@drawable/oliveapp_camera_bg_pressed</item>
    </style>
    <style name="SettingItemTitle">
        <item name="android:textSize">@dimen/oliveapp_camera_setting_item_text_size</item>
        <item name="android:textColor">@color/oliveapp_camera_primary_text</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="SettingPopupWindow">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/oliveapp_camera_setting_popup_right_margin</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="SettingRow">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/oliveapp_camera_setting_picker</item>
        <item name="android:paddingLeft">@dimen/oliveapp_camera_setting_item_list_margin</item>
        <item name="android:paddingRight">@dimen/oliveapp_camera_setting_item_list_margin</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/oliveapp_camera_setting_row_height</item>
    </style>
    <style name="SwitcherButton">
        <item name="android:background">@drawable/oliveapp_camera_bg_pressed_exit_fading</item>
        <item name="android:layout_width">@dimen/oliveapp_camera_switcher_size</item>
        <item name="android:layout_height">@dimen/oliveapp_camera_switcher_size</item>
    </style>
    <style name="TextAppearance" parent="@miui:style/TextAppearance">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginLeft">12.0dip</item>
        <item name="android:layout_marginRight">12.0dip</item>
    </style>
    <style name="TextAppearance.Hint" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/text_size_content_hint</item>
        <item name="android:textColor">@color/text_color_black_alpha_30</item>
    </style>
    <style name="TextAppearance.Hint.Link" parent="@style/TextAppearance.Hint">
        <item name="android:textSize">@dimen/text_size_link</item>
        <item name="android:textColor">@color/text_color_black</item>
    </style>
    <style name="TextAppearance.Title" parent="@style/TextAppearance">
        <item name="android:textSize">19.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:paddingLeft">20.0dip</item>
        <item name="android:paddingRight">20.0dip</item>
    </style>
    <style name="TextAppearance.Title.NavigatorView" parent="@style/TextAppearance.Title">
        <item name="android:textSize">@dimen/text_size_navigator_view_title_normal</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="TextAppearanceBigFont" parent="@style/TextAppearance">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/text_color_black2</item>
    </style>
    <style name="TextAppearanceBigFontGray" parent="@style/TextAppearance">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/text_color_gray99</item>
    </style>
    <style name="TextAppearanceWindowTitle" parent="@miui:style/TextAppearance.WindowTitle">
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="Theme.Camera" parent="@android:style/Theme.NoTitleBar.Fullscreen" />
    <style name="Theme.DiagnosisTitle" parent="@miui:style/Theme.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:actionBarStyle">@style/MainActionbarStyle</item>
        <item name="miui:windowTranslucentStatus">transparentDark</item>
    </style>
    <style name="Theme.Light.Immersion" parent="@miui:style/Theme.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/ActionBarWhiteTitle</item>
        <item name="miui:immersionMenuEnabled">true</item>
        <item name="miui:windowTranslucentStatus">transparentDark</item>
    </style>
    <style name="Theme.Light.Search" parent="@miui:style/Theme.Light.ActionBar.EmbedTabs">
        <item name="android:actionBarStyle">@style/ActionBar_Search_Dark</item>
        <item name="miui:editTextSearchStyle">@style/actionbar_search_text</item>
    </style>
    <style name="Theme.Light.WhiteActionBar" parent="@miui:style/Theme.Light">
        <item name="android:actionBarStyle">@style/ActionBarWhiteTitle</item>
        <item name="android:homeAsUpIndicator">@miui:drawable/action_bar_back_dark</item>
        <item name="miui:windowTranslucentStatus">transparentDark</item>
    </style>
    <style name="Theme.Main" parent="@miui:style/Theme.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:actionBarStyle">@style/MainActionbarStyle</item>
        <item name="miui:windowTranslucentStatus">transparentDark</item>
    </style>
    <style name="Theme.Main.NoTitile.TransparentDark" parent="@miui:style/Theme.Light.NoTitle">
        <item name="android:windowBackground">@color/bg_color</item>
        <item name="miui:windowTranslucentStatus">transparentDark</item>
    </style>
    <style name="Theme.Main.NoTitile.TransparentLight" parent="@miui:style/Theme.Light.NoTitle">
        <item name="android:windowBackground">@color/bg_color</item>
        <item name="miui:windowTranslucentStatus">transparentLight</item>
    </style>
    <style name="Theme.ProxyLauncher" parent="@android:style/Theme.Translucent.NoTitleBar" />
    <style name="Transparent" parent="@miui:style/Theme.Light.NoTitle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="Widget.Button" parent="@miui:style/Widget.Button">
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:minHeight">@dimen/main_button_min_height</item>
    </style>
    <style name="Widget.RatingBar.Edit" parent="@miui:style/Widget.RatingBar.Large">
        <item name="android:numStars">5</item>
        <item name="android:stepSize">1.0</item>
    </style>
    <style name="actionbar_search_text" parent="@miui:style/Widget.EditText.Search">
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="myProgressBarStyle">
        <item name="android:maxWidth">43.599976dip</item>
        <item name="android:maxHeight">43.599976dip</item>
        <item name="android:indeterminateDrawable">@drawable/mm_loading_progress</item>
        <item name="android:minWidth">43.599976dip</item>
        <item name="android:minHeight">43.599976dip</item>
    </style>
    <style name="simInfoItemViewDividerStyle" parent="@style/simInfoItemViewFirstStyle">
        <item name="android:background">#ffdfdfdf</item>
        <item name="android:layout_height">1.0px</item>
        <item name="android:layout_marginLeft">@dimen/page_margin_medium</item>
        <item name="android:layout_marginRight">@dimen/page_margin_medium</item>
        <item name="android:minHeight">1.0px</item>
    </style>
    <style name="simInfoItemViewExpandableStyle" parent="@style/simInfoItemViewFirstStyle">
        <item name="android:background">@null</item>
    </style>
