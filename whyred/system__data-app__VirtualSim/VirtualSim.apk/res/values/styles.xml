        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/sim_item_bg_first</item>
        <item name="android:paddingLeft">@dimen/page_margin_medium</item>
        <item name="android:paddingTop">5.0dip</item>
        <item name="android:paddingRight">@dimen/page_margin_medium</item>
        <item name="android:paddingBottom">5.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/sim_info_item_height</item>
    </style>
    <style name="simInfoItemViewLastStyle" parent="@style/simInfoItemViewFirstStyle">
        <item name="android:background">@drawable/sim_item_bg_last</item>
    </style>
    <style name="simInfoSubTextStyle" parent="@style/simInfoSubTitleStyle">
        <item name="android:textColor">@color/sim_info_sub_text_color</item>
        <item name="android:paddingLeft">@dimen/sim_info_label_padding_left_large</item>
    </style>
    <style name="simInfoSubTitleStyle">
        <item name="android:textSize">@dimen/sim_info_sub_title_size</item>
        <item name="android:textColor">@color/sim_info_sub_title_color</item>
        <item name="android:textColorLink">@color/sim_info_headline_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textIsSelectable">true</item>
    </style>
    <style name="simInfoTitleStyle">
        <item name="android:textSize">@dimen/sim_info_title_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/sim_info_title_color</item>
        <item name="android:textColorLink">@color/sim_info_headline_color</item>
        <item name="android:autoLink">all</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="simInfoTitleTextStyle" parent="@style/simInfoTitleStyle">
        <item name="android:textSize">@dimen/sim_info_title_text_size</item>
        <item name="android:textColor">@color/sim_info_title_text_color</item>
    </style>
</resources>
