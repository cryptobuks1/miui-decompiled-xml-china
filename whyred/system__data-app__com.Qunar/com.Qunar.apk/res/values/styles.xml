<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Payeco_MyDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/payeco_dia_bg</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="Theme.quicklogin" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="payeco_datepPickDialog" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="payeco_fullHeightDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="payeco_keyboardButton">
        <item name="android:textSize">@dimen/payeco_pwkeyboard_button_textsize</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">70.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="payeco_pluginNormalText">
        <item name="android:textSize">@dimen/payeco_normal_textsize</item>
        <item name="android:textColor">@color/payeco_textColorBlack</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="payeco_qunar_creditKeyboardDigitButton">
        <item name="android:textSize">@dimen/payeco_pwkeyboard_button_textsize</item>
        <item name="android:textColor">#ff0b0000</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/payeco_qunar_digtselector</item>
        <item name="android:layout_width">0.0px</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="pub_react_de_translucent" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/pub_react_de_color_transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>
    <style name="spider_AppBaseTheme" parent="@android:style/Theme.Light" />
    <style name="spider_ButtonWhite" parent="@android:style/Widget.Button">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">@color/spider_button_white_txcolor_selector</item>
        <item name="android:background">@drawable/spider_button_white_bg_selector</item>
    </style>
    <style name="spider_StyleCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:background">@drawable/spider_btn_check_label_background</item>
        <item name="android:button">?android:listChoiceIndicatorMultiple</item>
    </style>
    <style name="spider_StyleEditText" parent="@android:style/Widget.EditText">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:textColorHint">@color/spider_hint_color_selector</item>
        <item name="android:background">@drawable/spider_editview_bg</item>
    </style>
    <style name="spider_StyleListView" parent="@android:style/Widget.ListView.White">
        <item name="android:background">#ffffffff</item>
        <item name="android:fadingEdgeLength">10.0dip</item>
        <item name="android:listSelector">@drawable/spider_theme_list_item_bg</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:divider">#ffe1e1e1</item>
        <item name="android:dividerHeight">1.0dip</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <style name="spider_StyleTextView" parent="@android:style/Widget.TextView">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:textColorHint">@color/spider_hint_color_selector</item>
    </style>
    <style name="spider_Theme_App" parent="@style/spider_AppBaseTheme">
        <item name="android:buttonStyle">@style/spider_ButtonWhite</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:checkboxStyle">@style/spider_StyleCheckBox</item>
        <item name="android:editTextStyle">@style/spider_StyleEditText</item>
        <item name="android:listViewStyle">@style/spider_StyleListView</item>
        <item name="android:textViewStyle">@style/spider_StyleTextView</item>
        <item name="android:listChoiceIndicatorMultiple">@drawable/spider_checkbox_selector</item>
    </style>
    <style name="spider_Theme_App_Translucent" parent="@style/spider_Theme_App">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="spider_Theme_App_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="spider_Theme_Float" parent="@style/spider_AppBaseTheme">
        <item name="android:buttonStyle">@style/spider_ButtonWhite</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:checkboxStyle">@style/spider_StyleCheckBox</item>
        <item name="android:editTextStyle">@style/spider_StyleEditText</item>
        <item name="android:listViewStyle">@style/spider_StyleListView</item>
        <item name="android:textViewStyle">@style/spider_StyleTextView</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:listChoiceIndicatorMultiple">@drawable/spider_checkbox_selector</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
        <item name="android:colorBackgroundCacheHint">@android:color/transparent</item>
    </style>
</resources>
