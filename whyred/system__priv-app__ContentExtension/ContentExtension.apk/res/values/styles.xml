<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomWindowTitleBackground">
        <item name="android:background">#ff565656</item>
    </style>
    <style name="Miui.Button" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">@style/Miui.Button.TextAppearance</item>
        <item name="android:textColor">@color/button_text_color_light</item>
        <item name="android:background">@drawable/btn_bg_light</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="Miui.Button.TextAppearance" parent="@style/Miui.Button">
        <item name="android:textSize">@dimen/button_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/button_text_color_light</item>
    </style>
    <style name="MiuiShare.TextAppearance.ShareItem" parent="@miui:style/TextAppearance.List.Secondary">
        <item name="android:textColor">@color/list_secondary_text_light</item>
    </style>
    <style name="MiuiShare.Theme.Light.Alert" parent="@miui:style/Theme.Light.Dialog.Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="MiuiShare.Theme.NoDisplay" parent="@miui:style/Theme.NoDisplay" />
    <style name="MiuiShare.Widget.Button.Dialog.Default" parent="@miui:style/Widget.Button.Dialog.Default" />
    <style name="MiuiShare.Widget.ButtonBar" parent="@miui:style/Widget.ButtonBar" />
    <style name="OperationItemType">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/segment_operation</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="ProcessorTheme" parent="@miui:style/Theme.Dark.NoTitle">
        <item name="android:windowBackground">@color/translucent_background</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowSoftInputMode">stateAlwaysHidden|adjustPan</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="SegmentItemType">
        <item name="android:textSize">@dimen/segment_text_size</item>
        <item name="android:textColor">@color/segment_text_unselected</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ThumbnailDescriptionStyle">
        <item name="android:textSize">@dimen/thumbnail_subtitle</item>
        <item name="android:textColor">@color/recommendation_card_subtitle</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">4.0dip</item>
        <item name="android:paddingRight">4.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ali_auth_qr_activity_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="alibc_auth_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/alibc_transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:background">@color/alibc_transparent</item>
        <item name="android:height">268.0dip</item>
        <item name="android:width">268.0dip</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="com_taobao_tae_sdk_pailitao_LoadingDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="com_taobao_tae_sdk_pailitao_notitle" parent="@android:style/Theme">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
    </style>
    <style name="feedback_title" parent="@android:style/Theme">
        <item name="android:windowTitleSize">50.0dip</item>
        <item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
    </style>
</resources>
