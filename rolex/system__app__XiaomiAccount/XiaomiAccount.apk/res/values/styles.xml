<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ProvisionHorizontalMargin">
        <item name="android:layout_marginStart">@dimen/passport_miui_provision_login_reg_margin_h</item>
        <item name="android:layout_marginEnd">@dimen/passport_miui_provision_login_reg_margin_h</item>
    </style>
    <style name="ProvisionActionBarTitleStyle">
        <item name="android:textSize">@dimen/passport_miui_provision_action_bar_title_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_marginTop">@dimen/passport_miui_provision_action_bar_title_margin_top</item>
        <item name="android:layout_marginStart">@dimen/passport_miui_provision_action_bar_title_margin_left</item>
    </style>
    <style name="ProvisionEditViewStyle" parent="@style/miui.Widget.EditText">
        <item name="android:textSize">@dimen/miui_title_text_size</item>
        <item name="android:textColorHint">@color/passport_miui_provision_edit_text_hint_color</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@null</item>
        <item name="android:layout_height">@dimen/passport_miui_provision_edit_text_height</item>
        <item name="android:paddingEnd">@dimen/passport_miui_provision_edit_text_padding_right</item>
    </style>
    <style name="ButtonNormal" parent="@style/miui.Widget.Button" />
    <style name="ButtonHighlight" parent="@style/miui.Widget.Button.Warning" />
    <style name="TextSecondPrimary" parent="@style/miui.TextAppearance.Medium" />
    <style name="HorizontalDivider">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/miui_divider</item>
        <item name="android:color">@color/miui_divider_line</item>
    </style>
    <style name="LoginRegTopPromptStyle" parent="@style/miui.TextAppearance.WindowTitle">
        <item name="android:textStyle">normal</item>
        <item name="android:layout_marginBottom">@dimen/passport_login_prompt_margin_bottom</item>
    </style>
    <style name="LoginRegLayoutPadding">
        <item name="android:paddingTop">@dimen/passport_login_reg_padding_top</item>
        <item name="android:paddingBottom">@dimen/passport_login_reg_padding_bottom</item>
        <item name="android:paddingStart">@dimen/passport_login_reg_padding_h</item>
        <item name="android:paddingEnd">@dimen/passport_login_reg_padding_h</item>
    </style>
    <style name="DialogLayoutPadding">
        <item name="android:paddingTop">@dimen/miui_dialog_title_vertical_padding</item>
        <item name="android:paddingBottom">@dimen/miui_dialog_title_vertical_padding</item>
        <item name="android:paddingStart">@dimen/miui_dialog_title_horizontal_padding</item>
        <item name="android:paddingEnd">@dimen/miui_dialog_title_horizontal_padding</item>
    </style>
    <style name="LoginRegGuideTextStyle">
        <item name="android:textSize">@dimen/passport_login_reg_guide_text_size</item>
        <item name="android:textColor">@color/passport_login_reg_guide_text_color</item>
    </style>
    <style name="ErrorNoticeAppearance">
        <item name="android:textSize">@dimen/miui_button_text_size</item>
        <item name="android:textColor">@color/passport_text_color_warn</item>
    </style>
    <style name="ContentLayoutPadding">
        <item name="android:layout_marginTop">@dimen/passport_content_vertical_margin</item>
        <item name="android:layout_marginBottom">@dimen/passport_content_bottom_margin</item>
        <item name="android:layout_marginStart">@dimen/passport_content_horizontal_margin</item>
        <item name="android:layout_marginEnd">@dimen/passport_content_horizontal_margin</item>
    </style>
    <style name="miui" />
    <style name="miui.TextAppearance" parent="@miui:style/TextAppearance" />
    <style name="miui.TextAppearance.Large" parent="@miui:style/TextAppearance.Large" />
