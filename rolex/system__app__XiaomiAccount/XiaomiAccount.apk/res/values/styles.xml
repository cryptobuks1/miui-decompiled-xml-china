    <style name="miui.TextAppearance.Medium" parent="@miui:style/TextAppearance.Medium" />
    <style name="miui.TextAppearance.PreferenceList" parent="@miui:style/TextAppearance.PreferenceList" />
    <style name="miui.TextAppearance.Widget.Button" parent="@miui:style/TextAppearance.Widget.Button" />
    <style name="miui.TextAppearance.Widget.List.Primary" parent="@miui:style/TextAppearance.Widget.List.Primary" />
    <style name="miui.TextAppearance.Widget.List.Secondary" parent="@miui:style/TextAppearance.Widget.List.Secondary" />
    <style name="miui.Theme" parent="@style/miui" />
    <style name="miui.Theme.Light" parent="@miui:style/Theme.Light" />
    <style name="miui.Theme.Light.ActionBar.NoTitle" parent="@miui:style/Theme.Light.ActionBar.NoTitle" />
    <style name="miui.Theme.Light.Dialog" parent="@miui:style/Theme.Light.Dialog" />
    <style name="miui.Theme.Light.Dialog.Alert" parent="@miui:style/Theme.Light.Dialog.Alert">
        <item name="miui:windowActionBar">false</item>
    </style>
    <style name="miui.Theme.Light.Dialog.NoTitle" parent="@style/miui.Theme.Light.Dialog">
        <item name="miui:windowActionBar">false</item>
    </style>
    <style name="miui.Theme.Light.Settings" parent="@miui:style/Theme.Light.Settings" />
    <style name="miui.Theme.NoDisplay" parent="@miui:style/Theme.NoDisplay" />
    <style name="miui.Widget.ActionBar" parent="@miui:style/Widget.ActionBar" />
    <style name="miui.Widget.Button" parent="@miui:style/Widget.Button" />
    <style name="miui.Widget.Button.Negative" parent="@miui:style/Widget.Button.Negative" />
    <style name="miui.Widget.Button.Rect.BlankPage" parent="@miui:style/Widget.Button.Rect.BlankPage" />
    <style name="miui.Widget.Button.Warning" parent="@miui:style/Widget.Button.Warning" />
    <style name="miui.Widget.DialogTitle" parent="@miui:style/Widget.DialogTitle" />
    <style name="miui.Widget.PreferenceItem" parent="@miui:style/Widget.PreferenceItem" />
    <style name="miui.Widget.EditText" parent="@miui:style/Widget.EditText">
        <item name="android:background">@drawable/passport_group_single_item_normal_bg</item>
    </style>
    <style name="miui.Widget.Button.Positive" parent="@miui:style/Widget.Button.Positive" />
    <style name="miui.TextAppearance.WindowTitle" parent="@miui:style/TextAppearance.WindowTitle" />
    <style name="miui.Widget.ListView.Item.SingleLine" parent="@miui:style/Widget.ListView.Item.SingleLine" />
    <style name="Theme.Main" parent="@style/miui.Theme.Light" />
    <style name="Theme.Dialog" parent="@style/miui.Theme.Light.Dialog" />
    <style name="Theme.Dialog.NoTitle" parent="@style/miui.Theme.Light.Dialog.NoTitle" />
    <style name="Theme.Dialog.NoTitle.Translucent" parent="@style/miui.Theme.Light.Dialog.NoTitle">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="Theme.NoDisplay" parent="@style/miui.Theme.NoDisplay" />
    <style name="Theme.Settings" parent="@style/miui.Theme.Light.Settings" />
    <style name="Theme.AreaCode" parent="@style/Theme.Main" />
    <style name="HyperLinkButtonStyle" parent="@style/miui.TextAppearance.Medium">
        <item name="android:textColor">@color/miui_action_bar_title_text_color_light</item>
    </style>
    <style name="TextPrimaryWelcome" parent="@style/miui.TextAppearance.Large" />
    <style name="ButtonRemove" parent="@style/miui.Widget.Button.Negative" />
    <style name="WeiBo.ButtonUpdateContacts" parent="@style/miui.Widget.Button.Warning" />
    <style name="TextAppearance.WeiBo" parent="@style/miui.TextAppearance" />
    <style name="TextAppearance.WeiBo.Description" parent="@style/TextAppearance.WeiBo">
        <item name="android:textSize">@dimen/miui_normal_text_size</item>
        <item name="android:textColor">@color/passport_text_color_gray</item>
    </style>
    <style name="TextAppearance.WeiBo.Primary" parent="@style/TextAppearance.WeiBo">
        <item name="android:textSize">@dimen/miui_large_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/miui_preference_primary_text_color_light</item>
    </style>
    <style name="TextAppearance.Device.Info1" parent="@style/miui.TextAppearance.Medium">
        <item name="android:textColor">@color/device_info1_text_color</item>
    </style>
    <style name="TextAppearance.Title.List.Primary" parent="@style/miui.TextAppearance.List.Primary">
        <item name="android:textColor">@color/no_network_text_light</item>
    </style>
    <style name="EmailNoticeTextAppearance" parent="@style/miui.TextAppearance.Large">
        <item name="android:color">@color/passport_primary_text_color</item>
    </style>
    <style name="ActivateEmailTextAppearance" parent="@style/miui.TextAppearance.Widget.Button">
        <item name="android:color">@color/passport_text_color_gray</item>
    </style>
    <item type="style" name="APKTOOL_DUMMY_3d">false</item>
