        <item name="android:actionBarItemBackground">@android:color/transparent</item>
    </style>
    <style name="Theme.Light.Immersion" parent="@miui:style/Theme.Light">
        <item name="miui:immersionMenuEnabled">true</item>
    </style>
    <style name="Theme.Light.NoTitile.Transparent" parent="@miui:style/Theme.Light.NoTitle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="Theme.Light.Immersion_Setting" parent="@miui:style/Theme.Light.Settings">
        <item name="miui:immersionMenuEnabled">true</item>
    </style>
</resources>
