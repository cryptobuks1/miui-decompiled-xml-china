<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionButton" parent="@miui:style/Widget.Button.Rect.BlankPage">
        <item name="android:layout_width">@dimen/suggestion_action_button_width</item>
        <item name="android:layout_height">@dimen/suggestion_action_button_height</item>
    </style>
    <style name="Animation.SelectSearchSource" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/corpus_selector_open</item>
        <item name="android:windowExitAnimation">@anim/corpus_selector_close</item>
    </style>
    <style name="BannerLabelTextStyle">
        <item name="android:textSize">@dimen/label_text_size</item>
        <item name="android:textColor">@color/banner_label_text_color</item>
    </style>
    <style name="ClassIndexTextStyle">
        <item name="android:textSize">@dimen/suggestion_tab_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/suggestion_tab_text_color</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ClearHistory">
        <item name="android:textSize">@dimen/suggestion_group_name_size</item>
        <item name="android:textColor">@color/more_text_color</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="CorpusName">
        <item name="android:textSize">@dimen/suggestion_group_name_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text2color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">start|center</item>
        <item name="android:layout_gravity">start|center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="DoubleLineSuggestionHint">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="FileListItemStyle">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/list_preferred_item_height</item>
        <item name="android:minHeight">@dimen/list_preferred_item_height</item>
        <item name="android:paddingStart">14.0dip</item>
        <item name="android:paddingEnd">14.0dip</item>
    </style>
    <style name="FilePickerTheme" parent="@style/Theme.QuickSearchBox">
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="HotWordTagText">
        <item name="android:textSize">@dimen/hot_word_tag_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/hot_word_tag_color</item>
    </style>
    <style name="HotwordLabelTextStyle">
        <item name="android:textSize">@dimen/label_text_size</item>
        <item name="android:textColor">@color/hotword_label_text_color</item>
    </style>
    <style name="IndicatedBarSingleTextStyle">
        <item name="android:textSize">@dimen/suggestion_group_name_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/view_pager_unselected_text_color</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="MiuiShare.TextAppearance.ShareItem" parent="@miui:style/TextAppearance.List.Secondary">
        <item name="android:textColor">@color/list_secondary_text_light</item>
    </style>
    <style name="MiuiShare.Theme.Light.Alert" parent="@miui:style/Theme.Light.Dialog.Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="MiuiShare.Widget.Button.Dialog.Default" parent="@miui:style/Widget.Button.Dialog" />
    <style name="MiuiShare.Widget.ButtonBar" parent="@miui:style/Widget.ButtonBar" />
    <style name="NetworkUnavailable">
        <item name="android:textSize">@dimen/network_unavailable_text_size</item>
        <item name="android:textColor">@color/network_unavailable_text_color</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="PathItemTextStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/path_item_text_color</item>
    </style>
    <style name="PromptCardButtonText">
        <item name="android:textSize">@dimen/prompt_card_button_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/prompt_card_positive_color</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">0.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">@dimen/prompt_card_button_height</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="PullToInputTextStyle">
        <item name="android:textSize">@dimen/pull_to_input_text_size</item>
        <item name="android:textColor">@color/pull_to_input_text_color_dark</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginStart">@dimen/pull_to_input_gap</item>
    </style>
    <style name="RatingBar" parent="@miui:style/Widget.RatingBar.Small">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">start|center</item>
        <item name="android:focusable">false</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:numStars">5</item>
        <item name="android:stepSize">0.5</item>
        <item name="android:isIndicator">true</item>
    </style>
    <style name="SearchHistoryText">
        <item name="android:textSize">@dimen/history_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text1color</item>
        <item name="android:gravity">start|center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="Suggestion">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TextAppearance">
        <item name="android:textSize">@dimen/secondary_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColorHighlight">?android:textColorHighlight</item>
        <item name="android:textColorHint">?android:textColorHint</item>
        <item name="android:textColorLink">?android:textColorLink</item>
    </style>
    <style name="TextAppearance.List" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/normal_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="TextAppearance.List.Primary" parent="@style/TextAppearance.List" />
    <style name="TextAppearance.List.Secondary" parent="@style/TextAppearance.List">
        <item name="android:textSize">@dimen/secondary_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="TextAppearance.List.Secondary.Preference" parent="@style/TextAppearance.List.Secondary" />
    <style name="TextPicker" parent="@android:style/Widget">
        <item name="android:textColorHighlight">@color/numberpicker_highlight_text</item>
        <item name="android:textColorHint">@color/numberpicker_hint_text</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@null</item>
        <item name="android:labelTextSize">@dimen/numberpicker_label_text_size</item>
        <item name="labelTextColor">@color/numberpicker_label</item>
        <item name="textSizeHighlight">@dimen/numberpicker_text_size_highlight</item>
        <item name="textSizeHint">@dimen/numberpicker_text_size_hint</item>
        <item name="labelPadding">@dimen/numberpicker_label_padding</item>
    </style>
    <style name="Theme.FavorList" parent="@miui:style/Theme.Light.ActionBar.EmbedTabs">
        <item name="android:displayOptions">homeAsUp|showCustom|showTitle</item>
    </style>
    <style name="Theme.HistoryList" parent="@style/Theme.FavorList">
        <item name="android:customNavigationLayout">@layout/history_custom_action_bar</item>
    </style>
    <style name="Theme.Light.Immersion" parent="@miui:style/Theme.Light.NoTitle">
        <item name="android:windowBackground">@android:color/white</item>
        <item name="miui:immersionMenuEnabled">true</item>
        <item name="miui:windowTranslucentStatus">transparentLight</item>
    </style>
    <style name="Theme.QuickSearchBox" parent="@miui:style/Theme.Light" />
    <style name="Theme.QuickSearchBox.Search" parent="@style/Theme.QuickSearchBox">
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/WindowsAnimation</item>
        <item name="android:windowDisablePreview">false</item>
        <item name="android:windowSoftInputMode">adjustNothing|stateUnchanged</item>
        <item name="android:overScrollMode">never</item>
        <item name="miui:windowActionBar">false</item>
    </style>
    <style name="Theme.SelectSearchSource" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/Animation.SelectSearchSource</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowSoftInputMode">adjustResize|stateUnchanged</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="WebviewTitleAppearance">
        <item name="android:textSize">@dimen/query_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorHint">@color/search_hint</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="WebviewTitleAppearance.Security" parent="@style/WebviewTitleAppearance">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#e6000000</item>
    </style>
    <style name="WindowsAnimation">
        <item name="android:activityOpenEnterAnimation">@anim/right_slide_in</item>
        <item name="android:activityOpenExitAnimation">@anim/left_slide_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/left_slide_in</item>
        <item name="android:activityCloseExitAnimation">@anim/right_slide_out</item>
        <item name="android:taskOpenEnterAnimation">@anim/right_slide_in</item>
        <item name="android:taskOpenExitAnimation">@anim/left_slide_out</item>
        <item name="android:taskCloseEnterAnimation">@anim/left_slide_in</item>
        <item name="android:taskCloseExitAnimation">@anim/right_slide_out</item>
        <item name="android:taskToFrontEnterAnimation">@anim/right_slide_in</item>
        <item name="android:taskToFrontExitAnimation">@anim/left_slide_out</item>
        <item name="android:taskToBackEnterAnimation">@anim/left_slide_in</item>
        <item name="android:taskToBackExitAnimation">@anim/right_slide_out</item>
    </style>
    <style name="recent_title_edit_style">
        <item name="android:textSize">@dimen/recent_edit_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text2color</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="recent_title_normal_style">
        <item name="android:textSize">@dimen/suggestion_group_name_size</item>
        <item name="android:textColor">@color/suggestion_group_name_color</item>
        <item name="android:gravity">start|center</item>
        <item name="android:layout_gravity">start|center</item>
        <item name="android:singleLine">true</item>
    </style>
</resources>
